{
  "name": "SlackSnap",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "16": "slack-snap.png",
    "128": "slack-snap.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js", "jquery.min.js"]
    }
  },
  "permissions": [
    "videoCapture",
    "*://*.slack.com/*"
  ]
}
